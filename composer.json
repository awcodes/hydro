{
    "name": "awcodes/filament-plugin",
    "description": "This is a command-line tool that replaces the need to use Filament's Plugin Skeleton through GitHub by allowing local setup of your plugin from the start.",
    "keywords": ["filament", "plugin", "console", "cli"],
    "homepage": "https://github.com/awcodes/filament-plugin",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/awcodes/filament-plugin",
        "source": "https://github.com/awcodes/filament-plugin"
    },
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-intl": "*",
        "ext-posix": "*",
        "laravel-zero/framework": "^10.0",
        "nunomaduro/termwind": "^1.15"
    },
    "require-dev": {
        "laravel/pint": "^1.5",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["filament-plugin"]
}
